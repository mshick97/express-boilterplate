    "url": "https://github.com/mshick97/express-boilterplate/issues"
  },
  "homepage": "https://github.com/mshick97/express-boilterplate#readme",
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.13",
    "supertest": "^7.1.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  }
